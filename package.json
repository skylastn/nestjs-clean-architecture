{
  "name": "automation-deploy",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node -r dotenv/config ./node_modules/.bin/nest start",
    "start:dev": "node -r dotenv/config ./node_modules/.bin/nest start --watch",
    "start:debug": "node -r dotenv/config ./node_modules/.bin/nest start --debug --watch",
    "start:prod": "node -r dotenv/config dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node -r dotenv/config ./node_modules/.bin/jest",
    "test:watch": "node -r dotenv/config ./node_modules/.bin/jest --watch",
    "test:cov": "node -r dotenv/config ./node_modules/.bin/jest --coverage",
    "test:debug": "node -r dotenv/config node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node -r dotenv/config ./node_modules/.bin/jest --config ./test/jest-e2e.json",
    "typeorm": "npx typeorm-ts-node-commonjs",
    "seed": "ts-node -r tsconfig-paths/register src/core/seeder/run_seeder.ts",
    "migrate": "npm run typeorm -- migration:run -d src/core/data_source.ts",
    "migrate:revert": "npm run typeorm -- migration:revert -d src/core/data_source.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.12",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/typeorm": "^11.0.0",
    "axios": "^1.13.4",
    "bcrypt": "^6.0.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "js-yaml": "^4.1.1",
    "mysql2": "^3.16.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.16",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.12",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.10",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^17.2.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
